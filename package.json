{
  "name": "smart-react-routing",
  "version": "1.0.2",
  "description": "A library to structure the routing logic in an app using react-router",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheGreenBeaver/smart-react-routing.git"
  },
  "files": [
    "/lib"
  ],
  "bin": {
    "init": "scripts/init.js",
    "add-page-group": "scripts/add-page-group.js",
    "add-page": "scripts/add-page.js"
  },
  "keywords": [],
  "author": "TheGreenBeaver",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "react": "^17.0.2",
    "react-router-dom": "^5.3.1",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-router-dom": "5.x"
  },
  "dependencies": {
    "query-string": "^7.1.1"
  }
}
